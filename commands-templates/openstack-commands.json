{
  "version": [
    "--version"
  ],
  "listnetworks": [
    "network",
    "list"
  ],
  "listservers": [
    "server",
    "list --project-domain <%= options['os-project-name'] %>"
  ],
  "listrouters": [
    "router",
    "list --project-domain <%= options['os-project-name'] %>"
  ],
  "listsecuritygroups": [
    "security group",
    "list --project-domain <%= options['os-project-name'] %>"
  ],
  "listsecuritygrouprules": [
    "security group rule",
    "list --all-projects"
  ],
  "listflavors": [
    "flavor",
    "list --all --long"
  ],
  "listimages": [
    "image",
    "list --public --sort id --status active"
  ],
  "liststacks": [
    "stack",
    "list"
  ],
  "showstack": [
    "stack",
    "show <%=options.stack_name%>"
  ],
  "liststackresources": [
    "stack",
    "resource list <%=options.stack_name%>"
  ],
  "createstack": [
    "stack create <%=options.stack_name%>",
    "--template <%=options.template%>"
  ],
  "deletestack": [
    "stack delete <%=options.stack_name%>"
  ],
  "liststacknodes": [
    "cluster node",
    "list --cluster <%=options.stack_name%>"
  ]
}
